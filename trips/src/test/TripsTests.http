POST http://localhost:8080/trips
Content-Type: application/json

{
  "origin": {
    "latitude": 1234.5678,
    "longitude": 1234.5678
  },
  "destination": {
    "latitude": 1.5678,
    "longitude": 1234.5678
  },
  "departureDate": "2022-12-12",
